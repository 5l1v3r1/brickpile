
@model String
@{
    var structureInfo = ViewData["StructureInfo"] as BrickPile.UI.IStructureInfo;
}           
@if (structureInfo.CurrentModel.Id == structureInfo.RootModel.Id) {
    @Html.TextBoxFor(_ => Model, new { disabled = "disabled" })
} else {
    @Html.TextBoxFor(_ => Model, new { @class = "slug" })
}
