
@model IQueryable<LitS3.ListEntry>

@{
    Layout = "~/Areas/Dashboard/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>


<ul>
    @foreach (var item in Model) {
        <li>
            @item.Name
        </li>
    }
</ul>

@section GlobalNavigation {
    <ul id="dashboard-nav">
        <li>@Html.ActionLink("Content","index","content")</li>
		<li>@Html.ActionLink("Users", "manageusers", "account", null, new { @class = "users" })</li>
        <li>@Html.ActionLink("Library", "index", "library")</li>
    </ul>
}

