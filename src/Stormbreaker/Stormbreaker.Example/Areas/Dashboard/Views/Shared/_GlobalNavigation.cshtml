
@model Stormbreaker.Dashboard.Web.Mvc.ViewModels.IDashboardViewModel
           
@using Stormbreaker.Dashboard.Web.Mvc.Html
@using Stormbreaker.Models

@(Html.Raw(Html.TreeView<IPageModel>(Model.CurrentModel,
    Model.StructureInfo,
